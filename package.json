{
  "name": "@chucky.cloud/sdk",
  "version": "0.3.1",
  "description": "Universal TypeScript SDK for Chucky - build Claude-powered AI assistants for browser and Node.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "tsx test/integration.test.ts",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "chucky",
    "claude",
    "anthropic",
    "ai",
    "sdk",
    "agent",
    "assistant",
    "llm",
    "mcp",
    "websocket",
    "ai-assistant",
    "chatbot",
    "tools",
    "streaming"
  ],
  "author": "Chucky Cloud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chucky-Cloud/chucky-sdk-typescript.git"
  },
  "homepage": "https://chucky.cloud",
  "bugs": {
    "url": "https://github.com/Chucky-Cloud/chucky-sdk-typescript/issues"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "@types/ws": "^8.5.13",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
